<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="book" catalog="beb" class="com.model.Book">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="order" catalog="beb" class="com.model.Order">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="shoplist" catalog="beb" class="com.model.Shoplist">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="user" catalog="beb" class="com.model.User">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="usertype" catalog="beb" class="com.model.Usertype">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="shoporder" catalog="beb" class="com.model.Shoporder">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>